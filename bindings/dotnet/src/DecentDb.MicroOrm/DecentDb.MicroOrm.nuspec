<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>DecentDb.MicroOrm</id>
    <version>$version$</version>
    <authors>DecentDb contributors</authors>
    <license type="expression">Apache-2.0</license>
    <description>DecentDB Micro-ORM (.NET 10) with bundled native engine</description>
    <icon>icon.png</icon>
    <readme>README.md</readme>
    <repository type="git" url="https://github.com/sphildreth/decentdb" />
    <dependencies>
      <group targetFramework="net10.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="README.md" />
    <file src="..\..\..\..\graphics\logo-256x256.png" target="icon.png" />

    <file src="bin\$configuration$\net10.0\DecentDb.MicroOrm.dll" target="lib\net10.0\" />
    <file src="..\DecentDb.AdoNet\bin\$configuration$\net10.0\DecentDb.AdoNet.dll" target="lib\net10.0\" />
    <file src="..\DecentDb.Native\bin\$configuration$\net10.0\DecentDb.Native.dll" target="lib\net10.0\" />

    <file src="runtimes\linux-x64\native\libdecentdb.so" target="runtimes\linux-x64\native\" />
    <file src="runtimes\osx-x64\native\libdecentdb.dylib" target="runtimes\osx-x64\native\" />
    <file src="runtimes\win-x64\native\decentdb.dll" target="runtimes\win-x64\native\" />
  </files>
</package>
